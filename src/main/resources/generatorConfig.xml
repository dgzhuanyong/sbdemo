<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  <classPathEntry location="F:\mysql-connector-java-5.1.6.jar" />
  <!-- defaultModelType="flat" 简单模式 只生成一个entity和dao   -->
  <context id="contextId" defaultModelType="flat"> 
  		<commentGenerator>  
            <property name="suppressAllComments" value="true" /><!-- 是否取消注释 -->  
            <property name="suppressDate" value="false" /> <!-- 是否生成注释代时间戳 -->  
        </commentGenerator>
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://127.0.0.1:3306/yshs" userId="root" password="root" />
    
    <javaModelGenerator targetPackage="com.py.yshs.entity" targetProject="yshs/src/main/java" />
    
    <sqlMapGenerator targetPackage="mybatis.mapper" targetProject="yshs/src/main/resources" />
    
    <javaClientGenerator targetPackage="com.py.yshs.mapper" targetProject="yshs/src/main/java" type="XMLMAPPER" />
    
    <table domainObjectName="God" tableName="sys_god" enableCountByExample="false" enableUpdateByExample="false"  
            enableDeleteByExample="false" enableSelectByExample="false"  
            selectByExampleQueryId="false">
    </table>
    
    
    
  </context>
</generatorConfiguration>